  <script>
    $('.btn-edit').on('click', function(){
      var nama = $(this).data('nama');
      var foto = $(this).data('foto');
      var nis = $(this).data('nis');
      var nisn = $(this).data('nisn');

      var tempat_lahir = $(this).data('tempat_lahir');
      var tanggal_lahir = $(this).data('tanggal_lahir');
      var alamat = $(this).data('alamat');

      var url = '';
      if(foto == null || foto == ''){
        url = 'https://belajar.e-smanpul.com/img/no-image.jpeg';
      }else{
        url = 'https://belajar.e-smanpul.com/img/avatar_murid/'+foto;
      }

      $('#unama').val(nama);
      $('#unis').val(nis);
      $('#unisn').val(nisn);
      $('#utempat_lahir').val(tempat_lahir);
      $('#utanggal_lahir').val(tanggal_lahir);
      $('#utanggal_lahir').change();
      $('#ualamat').val(alamat);
      $('#imageku_src').attr('src', url);
      $('#profil').modal('show')
    })
  </script>
  <script>
    $('.btn-edit-pw').on('click', function(){
      var username = $(this).data('username');
      $('#uusername').val(username);
      $('#account').modal('show')
    })
  </script>
  <script>
    jQuery("#imageku").change(function(){
        readURLicon(this, '#imageku_src');
    });

    function readURLicon(input, tmp) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $(tmp).attr('src', e.target.result);
            }

            reader.readAsDataURL(input.files[0]);
        }
    }
</script>

</body>
